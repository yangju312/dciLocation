<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/layout_full"
    android:layout_width="180dp"
    android:layout_height="60dp"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context=".GISLocationActivity" >
  <RelativeLayout 
       android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bak">
    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
         >
         <RelativeLayout  android:id="@+id/LinearLayout1"
             android:layout_width="60dp"
                    android:layout_height="wrap_content"
                 
                    android:layout_weight="0.3">
          <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                 
                    android:layout_marginLeft="25dp"
                    android:contentDescription="@string/cx">
                </ImageView>
                <TextView
                    android:id="@+id/TextView1"
                    android:layout_width="wrap_content"
                    android:layout_below="@id/imageView8"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="@string/lsjlcx"
                    android:textColor="@color/txtcc"
                    android:textSize="15sp">
                </TextView>
                 
                </RelativeLayout>
                  <RelativeLayout android:id="@+id/LinearLayout2"
                       android:layout_width="60dp"
                    android:layout_height="wrap_content"
                 
                    android:layout_weight="0.3">
          <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="wrap_content"
                     android:layout_marginLeft="25dp"
                    android:layout_height="wrap_content"
                
                    android:contentDescription="@string/cx">
                </ImageView>
                 <TextView
                    android:id="@+id/TextView2"
                   android:layout_below="@id/imageView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/xtsz"
                    android:textSize="15sp"
                    android:textColor="@color/txtcc"
                   android:layout_marginLeft="10dp"
                    >
                </TextView>
                 </RelativeLayout>
                  <RelativeLayout android:id="@+id/LinearLayout3"
                      android:layout_width="60dp"
                    android:layout_height="wrap_content"
                 
                    android:layout_weight="0.3">
                 <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                   
                     android:layout_marginLeft="25dp"
                    android:contentDescription="@string/cx">
                </ImageView>
                <TextView
                    android:id="@+id/TextView3"
                    android:layout_width="wrap_content"
                   android:layout_marginLeft="10dp"
                    android:layout_below="@id/imageView2"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                   
                    android:gravity="center"
                     android:text="@string/tc"
                    android:textColor="@color/txtcc"
                   />
                
                 </RelativeLayout>
        
    </TableRow>
   
</RelativeLayout>
</TableLayout>
